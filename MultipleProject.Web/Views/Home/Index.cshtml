@model List<WeatherForecast>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome to DemoLab-Scrum - Features 2 - change</h1>
    <p>@ViewBag.Comment</p>

</div>

<div>
    @for (int i = 0; i < Model.Count; i++)
    {
        <p>Date : @Model[i].Date</p>
        <p>Summary : @Model[i].Summary</p>
        <p>Temperature : @Model[i].TemperatureC</p>
        <hr />
    }
</div>